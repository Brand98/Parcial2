<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Gps.Push">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Nombre"/>
            <Entry x:Name="Nombre"/>
            <Label Text="Titulo"/>
            <Entry x:Name="titulo"/>
            <Label Text="Mensaje"/>
            <Entry x:Name="mensaje"/>
            <Button Text="Enviar" Clicked="Button_Clicked_1"></Button>
            <Button Text="Volver" Clicked="Button_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>